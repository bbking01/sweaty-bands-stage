<?xml version="1.0"?>
<config>
	<modules>
		<Fishpig_Wordpress_Addon_WordPressSEO>
			<version>1.0.0</version>
		</Fishpig_Wordpress_Addon_WordPressSEO>
	</modules>
	<frontend>
		<events>
			<wordpress_render_layout_before>
				<observers>
					<wp_addon_wordpressSeo>
						<type>singleton</type>
						<class>Fishpig_Wordpress_Addon_WordPressSEO_Helper_Data</class>
						<method>injectSeoObserver</method>
					</wp_addon_wordpressSeo>
				</observers>
			</wordpress_render_layout_before>
		</events>
	</frontend>
</config>